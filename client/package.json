{
  "name": "client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html --log-level 3 --port 1234 --cache-dir .cache --open --out-dir dist",
    "build:parcelBuild": "parcel build ./src/index.html --no-source-maps --detailed-report | tee reports/parcel-build.txt",
    "build": "run-s clean build:parcelBuild build:distDirTree",
    "build:distDirTree": "tree dist -a -F -L 2",
    "serve": "run-s build run-prod-server",
    "run-prod-server": "cd dist; ws --spa --port 58975 -z",
    "prettier": "prettier --write \"./src/**/*.{js,jsx}\"",
    "test": "echo 'Testing TODO'",
    "deploy": "run-s build deploy:copyindex deploy:copyreports deploy:upload",
    "deploy:copyindex": "cp dist/index.html dist/200.html",
    "deploy:copyreports": "cp -rv reports dist/reports",
    "deploy:upload": "surge dist toptal-nsisodiya.surge.sh",
    "test1": "jest --coverage --updateSnapshot",
    "test:watch": "jest --watch",
    "lint:css": "stylelint \"./src/**/*.js\"",
    "lint": "eslint -c eslintrc.js ./src --ext .js,.jsx",
    "lint:fix": "eslint --fix -c eslintrc.js ./src --ext .js,.jsx",
    "prestart": "run-s clean",
    "clean": "rimraf dist .cche",
    "fix": "run-s prettier lint:fix lint",
    "gitadd": "git add .",
    "generateComponent": "node src/devtools/genComponent.js",
    "bkp-post-merge": "npm install --cache .npm --prefer-offline"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s fix test gitadd"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ant-design/icons": "4.2.2",
    "@fullhuman/postcss-purgecss": "2.3.0",
    "antd": "4.5.3",
    "chalk": "4.1.0",
    "change-case": "4.1.1",
    "css-template": "0.2.3",
    "enquirer": "2.3.6",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.5",
    "husky": "4.2.5",
    "immer": "6.0.9",
    "loopobject": "1.1.0",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.4",
    "postcss-modules": "3.2.0",
    "prettier": "2.0.5",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-is": "16.13.1",
    "react-json-viewer": "2.1.0",
    "react-redux": "7.2.1",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-wow": "1.0.0",
    "styled-components": "5.1.1",
    "tailwindcss": "1.6.2",
    "typeface-rubik": "0.0.72",
    "validator": "13.1.1"
  }
}
